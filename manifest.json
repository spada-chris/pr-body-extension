{
  "name": "Github PR Enhancer",
  "version": "0.0.1",
  "description": "Add the PR body to Github files view",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
      {
          "matches": ["https://github.com/*"],
          "js": ["content.js"],
          "run_at": "document_end"
      }
  ],
  "permissions": [
    "webNavigation"
  ]
}
